{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "lib": ["dom", "es2015", "es2017"],
      "strict": true,
      "noEmit": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "noUnusedLocals": false,
      "allowJs": true
    },
    "exclude": ["node_modules", "public", ".cache"]
  }